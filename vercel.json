{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "pressco21.vercel.app"
        }
      ],
      "destination": "https://hub.foreverlove.co.kr/:path*",
      "permanent": true
    }
  ],
  "crons": [
    {
      "path": "/api/cron/weekly-review",
      "schedule": "0 9 * * 0"
    }
  ]
}
